#Arithmetic_Operation


if __name__ == '__main__':
    a = int(input())
    b = int(input())

    
    # Calculate the sum of the two numbers
    print(a + b)
    
    # Calculate the difference of the two numbers (first - second)
    print(a - b)
    
    # Calculate the product of the two numbers
    print(a * b)

    
